<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefWidth="231.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.PamjaLogin">
    <children>
        <StackPane>
            <children>
                <VBox spacing="10.0">
                    <children>
                        <GridPane>
                            <children>
                                <Label text="Username" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                                <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                                <TextField fx:id="user" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                                <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                        <StackPane prefHeight="-1.0" prefWidth="-1.0">
                            <children>
                                <Button fx:id="loginButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Login" StackPane.alignment="CENTER" />
                            </children>
                        </StackPane>
                    </children>
                </VBox>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                          <Label fx:id="errorLabel" alignment="BOTTOM_LEFT" StackPane.alignment="BOTTOM_LEFT" />
               </children>
            </StackPane>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</StackPane>
